Private Sub InicializaApelidos(vgComOQue As Integer)
   On Error Resume Next                           'prepara para possíveis erros
   If (vgTb.RecordCount > 0 And vgTb.Eof = False And vgTb.Bof = False) Or vgComOQue = COM_TEXTBOX Then
      If vgComOQue = COM_TEXTBOX Then
         Sequencia_do_Cheque = txtCampo(0).Value
         Documento = txtCampo(1).Value
         Codigo_do_Banco = txtCampo(3).Value
         Sequencia_do_geral = txtCampo(6).Value
         Agencia = txtCampo(4).Value
         Data_da_entrada = txtCampo(2).Value
         Data_Boa = txtCampo(7).Value
         Valor_do_Cheque = txtCampo(5).Value
         Compensado = chkCampo(0).Value
         Data_da_Compessacao = txtCampo(8).Value
         Historico = txtCampo(11).Value
         Vendedor_Responsavel = txtCampo(9).Value
         Numero_do_cheque = txtCampo(10).Value
         Emitente_do_Cheque = txtCampo(12).Value
         Devolvido = chkCampo(1).Value
         Motivo_da_Devolucao = txtCampo(13).Value
      Else
         Sequencia_do_Cheque = IIf(IsNull(vgTb![Sequencia do Cheque]), 0, vgTb![Sequencia do Cheque])
         Documento = IIf(IsNull(vgTb!Documento), "", vgTb!Documento)
         Codigo_do_Banco = IIf(IsNull(vgTb![Codigo do Banco]), 0, vgTb![Codigo do Banco])
         Sequencia_do_geral = IIf(IsNull(vgTb![Sequencia do geral]), 0, vgTb![Sequencia do geral])
         Agencia = IIf(IsNull(vgTb!Agencia), "", vgTb!Agencia)
         Data_da_entrada = vgTb![Data da entrada]
         Data_Boa = vgTb![Data Boa]
         Valor_do_Cheque = IIf(IsNull(vgTb![Valor do Cheque]), 0, vgTb![Valor do Cheque])
         Compensado = IIf(IsNull(vgTb!Compensado), 0, vgTb!Compensado)
         Data_da_Compessacao = vgTb![Data da Compessação]
         Historico = IIf(IsNull(vgTb!Historico), "", vgTb!Historico)
         Vendedor_Responsavel = IIf(IsNull(vgTb![Vendedor Responsavel]), 0, vgTb![Vendedor Responsavel])
         Numero_do_cheque = IIf(IsNull(vgTb![Numero do cheque]), "", vgTb![Numero do cheque])
         Emitente_do_Cheque = IIf(IsNull(vgTb![Emitente do Cheque]), "", vgTb![Emitente do Cheque])
         Devolvido = IIf(IsNull(vgTb!Devolvido), 0, vgTb!Devolvido)
         Motivo_da_Devolucao = IIf(IsNull(vgTb![Motivo da Devolução]), "", vgTb![Motivo da Devolução])
      End If
   End If
   If Err Then Err.Clear                          'se deu algum erro, vamos resetá-lo
End Sub

